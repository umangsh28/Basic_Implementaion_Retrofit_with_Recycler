<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/sd"
        android:layout_width="169dp"
        android:layout_height="49dp"
        android:layout_marginTop="112dp"
        android:text="Show date"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.217"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/positive"
        android:layout_width="166dp"
        android:layout_height="47dp"
        android:layout_marginTop="40dp"
        android:text="positive"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.232"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/sd" />

    <TextView
        android:id="@+id/negative"
        android:layout_width="151dp"
        android:layout_height="49dp"
        android:layout_marginTop="52dp"
        android:text="Negative"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.219"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/positive" />

    <TextView
        android:id="@+id/hosp"
        android:layout_width="160dp"
        android:layout_height="53dp"
        android:layout_marginTop="44dp"
        android:text="hospitalize"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.227"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/negative" />

    <TextView
        android:id="@+id/death"
        android:layout_width="170dp"
        android:layout_height="52dp"
        android:layout_marginTop="40dp"
        android:text="Death"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.246"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/hosp" />

    <TextView
        android:id="@+id/datecheck"
        android:layout_width="170dp"
        android:layout_height="52dp"
        android:layout_marginTop="44dp"
        android:text="datecheck"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.282"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/death"
        app:layout_constraintVertical_bias="0.0" />
</androidx.constraintlayout.widget.ConstraintLayout>